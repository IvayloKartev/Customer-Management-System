generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/cms"
}

model User {
  id Int @id @unique @default(autoincrement())
  name String @unique
  email String @unique
  password String
  companies Company[] 
}

model Company {
  id Int @id @unique @default(autoincrement())
  name String @unique
  owner User @relation(fields: [ownerId], references: [id])
  ownerId Int @unique
}

